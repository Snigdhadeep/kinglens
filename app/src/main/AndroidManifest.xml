<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.king.king_lens">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="ADASAT"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Home_adslot"
            android:label="HOME"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Splashscreen"
            android:label="ADASAT"
            android:theme="@style/AppThemeSplash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EnterScreen"
            android:label="ADASAT"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LoginActivity"
            android:label="ADASAT.NET"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="adjustResize|stateHidden" />
        <activity
            android:name=".Grid_List.Item_activity"
            android:label="@string/title_activity_wishlist"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Grid_List.Gridlist_Activity"
            android:label="ADASAT.NET"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Grid_List.Filter_Activity" />
        <activity android:name=".Grid_List.Popup_Activity"
            android:theme="@style/AppTheme.popup"
            ></activity>
    </application>

</manifest>